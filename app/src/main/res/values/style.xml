<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/birujanda</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="SeekBar" parent="AppTheme">
        <item name="colorControlActivated">@color/white</item>
        <item name="colorControlNormal">@color/gray</item>
    </style>

    <style name="firstFont" parent="@android:style/TextAppearance.Small">
        <item name="android:fontFamily">@font/baloo</item>
    </style>
    <style name="secondFont" parent="@android:style/TextAppearance.Small">
        <item name="android:fontFamily">@font/montserrat</item>
    </style>
</resources>